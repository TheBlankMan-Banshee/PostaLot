{
  "name": "PostaLot",
  "version": "1.0.0",
  "description": "A React app for managing user contents and albums",
  "main": "server.js",
  "engines": {
    "node": "14.18.x",
    "npm": "6.x.x"
  },
  "dependencies": {
    "concurrently": "^6.3.0"
  },
  "scripts": {
    "start": "cd server && node server.js",
    "frontStart": "cd client && npm start",
    "frontSetup": "cd client && npm install && npm run build",
    "backStart": "cd server && npm start",
    "backSetup": "cd server && npm install",
    "prebuild": "concurrently \"npm run backSetup\" \"npm run frontSetup\"",
    "heroku-postbuild":"concurrently \"npm run backStart\" \"npm run frontStart\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheBlankMan-Banshee/PostaLot.git"
  },
  "keywords": [],
